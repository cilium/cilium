[{
  "endpointSelector": {"matchLabels": {"app": "service"}},
  "ingress": [{
    "fromEndpoints": [
      {"matchLabels": {"env": "prod"}}
    ],
    "toPorts": [{
      "ports": [
        {"port": "80", "protocol": "TCP"}
      ],
      "rules": {
        "HTTP": [
          {
            "method": "GET",
            "path": "/public"
          }
        ]
      }
    }]
  }]
}]
