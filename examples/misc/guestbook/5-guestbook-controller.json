{
    "kind":"ReplicationController",
    "apiVersion":"v1",
    "metadata":{
        "name":"guestbook",
        "labels":{
            "k8s-app.guestbook":"web"
        }
    },
    "spec":{
        "replicas":1,
        "selector":{
            "k8s-app.guestbook":"web"
        },
        "template":{
            "metadata":{
                "labels":{
                    "k8s-app.guestbook":"web"
                }
            },
            "spec":{
                "containers":[{
                    "name":"guestbook",
                    "image":"gcr.io/google-samples/gb-frontend:v4",
                    "ports":[{
                        "name":"http-server",
                        "containerPort": 80
                    }]
                }]
            }
        }
    }
}
